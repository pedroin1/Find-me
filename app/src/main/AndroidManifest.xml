<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tcc.find_me">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Find_me">
        <activity
            android:name=".Perfil.ChatActivity"
            android:label="@string/title_activity_chat2"
            android:theme="@style/Theme.Find_me.NoActionBar"></activity>
        <activity android:name=".config.ConfiguracaoProfissional" />
        <activity android:name=".config.ConfiguracaoCliente" />
        <activity android:name=".Perfil.ProfissionalActivity" />
        <activity android:name=".Perfil.ClienteActivity" />
        <activity
            android:name=".CadastroActivity"
            android:label="@string/titulo_cadastro" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/titulo_login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>